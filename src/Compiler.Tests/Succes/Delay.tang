import Nano

uint8 button
uint8 pinIN
uint8 pinOUT
uint8 led

button = 10
pinIN = 11
pinOUT = 12
led = 13

// Send signal to next chip
nothing Next()
    DigitalWrite(pinOUT, HIGH)
    for(uint16 i from 0 to 10000)
        // Nothing
    DigitalWrite(pinOUT, LOW)

PinModeD(led, OUTPUT)
PinModeD(pinOUT, OUTPUT)
PinModeD(pinIN, INPUT)
PinModeD(button, INPUT)

//PinModeD(pinIN, INPUT)

while(true)
    delay(1000)
    DigitalWrite(led, HIGH)
    delay(1000)
    DigitalWrite(led, LOW)
