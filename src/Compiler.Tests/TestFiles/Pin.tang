class DigitalPin
    register8 ddr
    register8 port
    register8 pin
    uint8 index
    nothing init(register8 ddrReg, register8 portReg, register8 pinReg, uint8 regIndex)
        ddr = ddrReg
        port = portReg
        pin = pinReg
        bitIndex = regIndex
    nothing digitalWrite(bool value)
        port{bitIndex} = value
    bool digitalRead()
        return pin{bitIndex}