import IO
import Pin

UnoPin UnoPin(uint8 pinNumber)
    UnoPin pin
    pin.init(pinNumber)
    return pin

class UnoPin
    Pin pin
    nothing init(uint8 pinNumber)
        if(pinNumber >= 0 and punNumber <= 7)
            pin.init(DDRD, PORTD, PIND, pinNumber)
        else if(pinNumber >= 8 and pinNumber <= 13)
            pin.init(DDRB, PORTB, PINB, pinNumber)
     

