[ //Dette er IKKE DEN RIGTIGE FIL, den, der ligger under docs er den rigtige.
    {
        "Name": "lineComment",
        "PatternString": "//.*",
        "Ignore": true
    },
    {
        "Name": "blockComment",
        "PatternString": "/\\*.*\\*/",
        "SingleLine": true,
        "Ignore": true
    },
    {
        "Name": "whitespace",
        "PatternString": "[\\t ]+",
        "Ignore": true
    },
    {
        "Name": "newLine",
        "PatternString": "\\r?\\n[\\r\\n\\t ]+",
        "SingleLine": true
    },
    {
        "Name": "intLiteral",
        "PatternString": "([+-]?(0|([1-9][0-9]*)))"
    },
    {
        "Name": "boolLiteral",
        "PatternString": "(true)|(false)"
    },
    {
        "Name": "strLiteral",
        "PatternString": "\"((\\\\\\\\)|(\\\\n)|(\\\\r)|(\\\\')|(\\\\\")|!|[#-&]|[\\(-\\[]|[\\]-~])*\""
    },
    {
        "Name": "charLiteral",
        "PatternString": "'((\\\\\\\\)|(\\\\n)|(\\\\r)|(\\\\')|(\\\\\")|!|[#-&]|[\\(-\\[]|[\\]-~])'"
    },
    {
        "Name": "if",
        "PatternString": "if"
    },
