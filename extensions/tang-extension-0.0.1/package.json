{
    "name": "Tang",
    "displayName": "tang-extension",
    "description": "Tang language",
    "version": "0.0.1",
    "publisher": "MortenRask",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "tang",
            "aliases": ["Tang", "tang"],
            "extensions": [".tang"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "tang",
            "scopeName": "source.tang",
            "path": "./syntaxes/tang.tmLanguage.json"
        }]
    }
}