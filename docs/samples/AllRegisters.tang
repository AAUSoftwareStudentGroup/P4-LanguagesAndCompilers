register8 ADCH   = register8(121)
uint16 ADC()
    uint16 result = 0
    for(int16 i from 0 to 7)
        if(ADCL{i})
            result = result + 2^(i)
    for(int16 i from 0 to 1)
        if(ADCH{i})
            result = result + 2^(8+i)
    return result
