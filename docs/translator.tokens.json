[
  {
    "Name": "lineComment",
    "PatternString": "//.*",
    "Ignore": true
  },
  {
    "Name": "blockComment",
    "PatternString": "/\\*.*\\*/",
    "SingleLine": true,
    "Ignore": true
  },
  {
    "Name": "whitespace",
    "PatternString": "[\\t ]+",
    "Ignore": true
  },
  {
    "Name": "newline",
    "PatternString": "\\r?\\n[\\r\\n\\t ]+",
    "SingleLine": true,
    "ignore": true
  },
  {
    "Name": "<=>",
    "PatternString": "\\<\\=\\>"
  },
  {
    "Name": "</>",
    "PatternString": "\\<\\/\\>"
  },
  {
    "Name": "%",
    "PatternString": "\\%"
  },
  {
    "Name": ":",
    "PatternString": "\\:"
  },
  {
    "Name": "[",
    "PatternString": "\\["
  },
  {
    "Name": "]",
    "PatternString": "\\]"
  },
  {
    "Name": "goto",
    "PatternString": "->"
  },
  {
    "Name": "insert",
    "PatternString": "<-"
  },
  {
    "Name": "symbol",
    "PatternString": "[\\!-9\\;-Z\\^-\\~]+"
  }
]
